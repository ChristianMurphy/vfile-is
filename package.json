{
  "name": "vfile-is",
  "version": "1.0.2",
  "description": "vfile utility to check if a vfile passes a test",
  "license": "MIT",
  "keywords": [
    "vfile",
    "vfile-util",
    "util",
    "utility",
    "virtual",
    "file",
    "text",
    "processing",
    "check",
    "is",
    "matches"
  ],
  "repository": "vfile/vfile-is",
  "bugs": "https://github.com/vfile/vfile-is/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "main": "index.js",
  "files": [
    "convert.js",
    "index.js"
  ],
  "dependencies": {
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "nyc": "^15.0.0",
    "prettier": "^2.0.0",
    "remark-cli": "^8.0.0",
    "remark-preset-wooorm": "^7.0.0",
    "tape": "^5.0.0",
    "to-vfile": "^6.0.0",
    "xo": "^0.32.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier . --write && xo --fix",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run format && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "guard-for-in": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
